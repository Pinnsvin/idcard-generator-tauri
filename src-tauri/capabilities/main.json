{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"app:default",
		"resources:default",
		"menu:default",
		"tray:default",
		"shell:allow-open",
		"updater:allow-check",
		"dialog:allow-open",
		"dialog:allow-save",
		"dialog:allow-confirm",
		"dialog:allow-message"
	]
}
