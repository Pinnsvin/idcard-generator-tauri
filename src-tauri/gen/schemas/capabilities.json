{
	"default": {
		"identifier": "default",
		"description": "Capability for the main window",
		"remote": null,
		"local": true,
		"windows": ["main"],
		"permissions": [
			"path:default",
			"event:default",
			"window:default",
			"app:default",
			"resources:default",
			"menu:default",
			"tray:default",
			"shell:allow-open",
			"updater:allow-check",
			"dialog:allow-open",
			"dialog:allow-save",
			"dialog:allow-confirm",
			"dialog:allow-message"
		],
		"platforms": ["linux", "macOS", "windows", "android", "iOS"]
	}
}
